<?xml version="1.0"?>

<config_pes>

  <!-- Default for un-recognized grid, machine, pesize, and / or compset -->
  <grid name="any">
    <mach name="any">
      <pes pesize="any" compset="_DATM.*_POP">
        <comment>default setting - used when no other match is found</comment>
        <!-- negative values mean number of nodes and are translated to number of tasks in the case -->
        <ntasks>
          <ntasks_ocn>-1</ntasks_ocn>
          <ntasks_ice>-1</ntasks_ice>
          <ntasks_cpl>-1</ntasks_cpl>
          <ntasks_wav>-1</ntasks_wav>
          <ntasks_atm>-1</ntasks_atm>
          <ntasks_lnd>-1</ntasks_lnd>
          <ntasks_rof>-1</ntasks_rof>
          <ntasks_glc>-1</ntasks_glc>
        </ntasks>
        <nthrds>
          <nthrds_atm>1</nthrds_atm>
          <nthrds_lnd>1</nthrds_lnd>
          <nthrds_rof>1</nthrds_rof>
          <nthrds_ice>1</nthrds_ice>
          <nthrds_ocn>1</nthrds_ocn>
          <nthrds_glc>1</nthrds_glc>
          <nthrds_wav>1</nthrds_wav>
          <nthrds_cpl>1</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_rof>0</rootpe_rof>
          <rootpe_ice>0</rootpe_ice>
          <rootpe_ocn>0</rootpe_ocn>
          <rootpe_glc>0</rootpe_glc>
          <rootpe_wav>0</rootpe_wav>
          <rootpe_cpl>0</rootpe_cpl>
        </rootpe>
      </pes>
    </mach>
  </grid>

  <!-- Defaults for gx3v7 resolution -->
  <grid name="oi%gx3v7">

    <!-- un-recognized machine, pesize, and / or compset  -->
    <mach name="any">
      <pes pesize="any" compset="_DATM.*_POP">
        <comment>none</comment>
        <ntasks>
          <ntasks_atm>16</ntasks_atm>
          <ntasks_lnd>16</ntasks_lnd>
          <ntasks_rof>16</ntasks_rof>
          <ntasks_ice>16</ntasks_ice>
          <ntasks_ocn>16</ntasks_ocn>
          <ntasks_cpl>16</ntasks_cpl>
          <ntasks_glc>16</ntasks_glc>
          <ntasks_wav>16</ntasks_wav>
        </ntasks>
        <nthrds>
          <nthrds_atm>2</nthrds_atm>
          <nthrds_lnd>2</nthrds_lnd>
          <nthrds_rof>2</nthrds_rof>
          <nthrds_ice>2</nthrds_ice>
          <nthrds_ocn>2</nthrds_ocn>
          <nthrds_glc>2</nthrds_glc>
          <nthrds_wav>2</nthrds_wav>
          <nthrds_cpl>2</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_rof>0</rootpe_rof>
          <rootpe_ice>0</rootpe_ice>
          <rootpe_ocn>0</rootpe_ocn>
          <rootpe_glc>0</rootpe_glc>
          <rootpe_wav>0</rootpe_wav>
          <rootpe_cpl>0</rootpe_cpl>
        </rootpe>
      </pes>
    </mach>

    <!-- un-recognized machine and / or compset, pesize = "T"  -->
    <mach name="any">
      <pes pesize="T" compset="_DATM.*_POP">
        <comment>none</comment>
        <ntasks>
          <ntasks_atm>8</ntasks_atm>
          <ntasks_rof>8</ntasks_rof>
          <ntasks_ice>5</ntasks_ice>
          <ntasks_ocn>8</ntasks_ocn>
          <ntasks_cpl>8</ntasks_cpl>
          <ntasks_lnd>8</ntasks_lnd>
          <ntasks_glc>4</ntasks_glc>
          <ntasks_wav>8</ntasks_wav>
        </ntasks>
        <nthrds>
          <nthrds_atm>4</nthrds_atm>
          <nthrds_lnd>4</nthrds_lnd>
          <nthrds_rof>4</nthrds_rof>
          <nthrds_ice>4</nthrds_ice>
          <nthrds_ocn>4</nthrds_ocn>
          <nthrds_glc>4</nthrds_glc>
          <nthrds_wav>4</nthrds_wav>
          <nthrds_cpl>4</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_rof>0</rootpe_rof>
          <rootpe_ice>0</rootpe_ice>
          <rootpe_ocn>0</rootpe_ocn>
          <rootpe_glc>0</rootpe_glc>
          <rootpe_wav>0</rootpe_wav>
          <rootpe_cpl>0</rootpe_cpl>
        </rootpe>
      </pes>
    </mach>

    <!-- gx3v7 resolution on yellowstone  -->
    <mach name="yellowstone">

      <!-- C, CECO, G, and GECO with gx3v7 on yellowstone:
           all components get 30x2 -->
      <pes pesize="any" compset="_DATM.*_POP">
        <comment>none</comment>
        <MAX_TASKS_PER_NODE>16</MAX_TASKS_PER_NODE>
        <PES_PER_NODE>16</PES_PER_NODE>
        <ntasks>
          <ntasks_atm>32</ntasks_atm>
          <ntasks_rof>32</ntasks_rof>
          <ntasks_ice>32</ntasks_ice>
          <ntasks_ocn>32</ntasks_ocn>
          <ntasks_cpl>32</ntasks_cpl>
          <ntasks_lnd>32</ntasks_lnd>
          <ntasks_glc>32</ntasks_glc>
          <ntasks_wav>32</ntasks_wav>
        </ntasks>
        <nthrds>
          <nthrds_atm>1</nthrds_atm>
          <nthrds_lnd>1</nthrds_lnd>
          <nthrds_rof>1</nthrds_rof>
          <nthrds_ice>1</nthrds_ice>
          <nthrds_ocn>1</nthrds_ocn>
          <nthrds_glc>1</nthrds_glc>
          <nthrds_wav>1</nthrds_wav>
          <nthrds_cpl>1</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_rof>0</rootpe_rof>
          <rootpe_ice>0</rootpe_ice>
          <rootpe_ocn>0</rootpe_ocn>
          <rootpe_glc>0</rootpe_glc>
          <rootpe_wav>0</rootpe_wav>
          <rootpe_cpl>0</rootpe_cpl>
        </rootpe>
      </pes>
    </mach>

    <!-- gx3v7 resolution on cheyenne  -->
    <mach name="cheyenne">

      <!-- C, CECO, G, and GECO with gx3v7 on cheyenne:
           all components get 32x2 -->
      <pes pesize="any" compset="_DATM.*_POP">
        <comment>none</comment>
        <ntasks>
          <ntasks_atm>32</ntasks_atm>
          <ntasks_rof>32</ntasks_rof>
          <ntasks_ice>32</ntasks_ice>
          <ntasks_ocn>32</ntasks_ocn>
          <ntasks_cpl>32</ntasks_cpl>
          <ntasks_lnd>32</ntasks_lnd>
          <ntasks_glc>32</ntasks_glc>
          <ntasks_wav>32</ntasks_wav>
        </ntasks>
        <nthrds>
          <nthrds_atm>2</nthrds_atm>
          <nthrds_lnd>2</nthrds_lnd>
          <nthrds_rof>2</nthrds_rof>
          <nthrds_ice>2</nthrds_ice>
          <nthrds_ocn>2</nthrds_ocn>
          <nthrds_glc>2</nthrds_glc>
          <nthrds_wav>2</nthrds_wav>
          <nthrds_cpl>2</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_rof>0</rootpe_rof>
          <rootpe_ice>0</rootpe_ice>
          <rootpe_ocn>0</rootpe_ocn>
          <rootpe_glc>0</rootpe_glc>
          <rootpe_wav>0</rootpe_wav>
          <rootpe_cpl>0</rootpe_cpl>
        </rootpe>
      </pes>
    </mach>

    <!-- gx3v7 resolution on hobart -->
    <mach name="hobart">

      <!-- C, CECO, G, and GECO with gx3v7 on gobart:
           all components get 24x1 -->
      <pes pesize="any" compset="_DATM.*_POP">
        <ntasks>
          <ntasks_atm>24</ntasks_atm>
          <ntasks_lnd>24</ntasks_lnd>
          <ntasks_rof>24</ntasks_rof>
          <ntasks_ice>24</ntasks_ice>
          <ntasks_ocn>24</ntasks_ocn>
          <ntasks_cpl>24</ntasks_cpl>
          <ntasks_glc>24</ntasks_glc>
          <ntasks_wav>24</ntasks_wav>
        </ntasks>
        <nthrds>
          <nthrds_atm>1</nthrds_atm>
          <nthrds_lnd>1</nthrds_lnd>
          <nthrds_rof>1</nthrds_rof>
          <nthrds_ice>1</nthrds_ice>
          <nthrds_ocn>1</nthrds_ocn>
          <nthrds_glc>1</nthrds_glc>
          <nthrds_wav>1</nthrds_wav>
          <nthrds_cpl>1</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_rof>0</rootpe_rof>
          <rootpe_ice>0</rootpe_ice>
          <rootpe_ocn>0</rootpe_ocn>
          <rootpe_glc>0</rootpe_glc>
          <rootpe_wav>0</rootpe_wav>
          <rootpe_cpl>0</rootpe_cpl>
        </rootpe>
      </pes>
    </mach>
  </grid>

  <!-- Defaults for gx1v6 or gx1v7 resolution -->
  <grid name="oi%gx1v6|oi%gx1v7">

    <!-- un-recognized machine and / or pesize:
           _POP and coupler get 2 tasks
           Ice (_DICE or _CICE) gets 4 tasks
           All other components get 1 task -->
    <mach name="any">
      <pes pesize="any" compset="_DATM.*_POP">
        <comment>none</comment>
        <ntasks>
          <ntasks_atm>1</ntasks_atm>
          <ntasks_rof>1</ntasks_rof>
          <ntasks_ocn>-2</ntasks_ocn>
          <ntasks_ice>-4</ntasks_ice>
          <ntasks_cpl>-2</ntasks_cpl>
          <ntasks_lnd>1</ntasks_lnd>
          <ntasks_glc>1</ntasks_glc>
          <ntasks_wav>1</ntasks_wav>
        </ntasks>
        <nthrds>
          <nthrds_atm>1</nthrds_atm>
          <nthrds_lnd>1</nthrds_lnd>
          <nthrds_rof>1</nthrds_rof>
          <nthrds_ice>1</nthrds_ice>
          <nthrds_ocn>1</nthrds_ocn>
          <nthrds_glc>1</nthrds_glc>
          <nthrds_wav>1</nthrds_wav>
          <nthrds_cpl>1</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_rof>0</rootpe_rof>
          <rootpe_ice>0</rootpe_ice>
          <rootpe_ocn>0</rootpe_ocn>
          <rootpe_cpl>0</rootpe_cpl>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_wav>0</rootpe_wav>
          <rootpe_glc>0</rootpe_glc>
        </rootpe>
      </pes>

      <!-- un-recognized machine and pesize = "S"
           _POP gets 480x1 by itself
           Ice (_DICE or _CICE) get 480x1 but shares with all below
           CPL gets 32x1
           DROF gets 16x1
           _DATM gets 16x1
           All other components get 1x1 (sharing with _DATM) -->
      <pes pesize="S" compset="_DATM.*_POP">
        <comment>none</comment>
        <ntasks>
          <ntasks_atm>16</ntasks_atm>
          <ntasks_rof>16</ntasks_rof>
          <ntasks_ice>480</ntasks_ice>
          <ntasks_ocn>480</ntasks_ocn>
          <ntasks_cpl>32</ntasks_cpl>
          <ntasks_lnd>1</ntasks_lnd>
          <ntasks_wav>1</ntasks_wav>
          <ntasks_glc>1</ntasks_glc>
        </ntasks>
        <nthrds>
          <nthrds_atm>1</nthrds_atm>
          <nthrds_lnd>1</nthrds_lnd>
          <nthrds_rof>1</nthrds_rof>
          <nthrds_ice>1</nthrds_ice>
          <nthrds_ocn>1</nthrds_ocn>
          <nthrds_glc>1</nthrds_glc>
          <nthrds_wav>1</nthrds_wav>
          <nthrds_cpl>1</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_rof>16</rootpe_rof>
          <rootpe_ice>0</rootpe_ice>
          <rootpe_ocn>480</rootpe_ocn>
          <rootpe_cpl>32</rootpe_cpl>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_wav>0</rootpe_wav>
          <rootpe_glc>0</rootpe_glc>
        </rootpe>
      </pes>

      <!-- un-recognized machine, G compset, pesize = "L"  -->
      <pes pesize="L" compset="_DATM.*_CICE.*_POP">
        <comment>none</comment>
        <ntasks>
          <ntasks_atm>1</ntasks_atm>
          <ntasks_rof>1</ntasks_rof>
          <ntasks_ocn>80</ntasks_ocn>
          <ntasks_ice>192</ntasks_ice>
          <ntasks_cpl>80</ntasks_cpl>
          <ntasks_lnd>1</ntasks_lnd>
          <ntasks_glc>1</ntasks_glc>
          <ntasks_wav>1</ntasks_wav>
        </ntasks>
        <nthrds>
          <nthrds_atm>1</nthrds_atm>
          <nthrds_lnd>1</nthrds_lnd>
          <nthrds_rof>1</nthrds_rof>
          <nthrds_ice>4</nthrds_ice>
          <nthrds_ocn>4</nthrds_ocn>
          <nthrds_glc>1</nthrds_glc>
          <nthrds_wav>1</nthrds_wav>
          <nthrds_cpl>1</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_rof>0</rootpe_rof>
          <rootpe_ice>0</rootpe_ice>
          <rootpe_ocn>80</rootpe_ocn>
          <rootpe_cpl>0</rootpe_cpl>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_glc>0</rootpe_glc>
          <rootpe_wav>0</rootpe_wav>
        </rootpe>
      </pes>
    </mach>

    <!-- gx1v6 or gx1v7 resolution on yellowstone  -->
    <mach name="yellowstone">

      <!-- any compset, gx1v6 or gx1v7 with pesize="T" on yellowstone:
           all components get 32x1 -->
      <pes pesize="T" compset="_DATM.*_POP">
        <MAX_TASKS_PER_NODE>16</MAX_TASKS_PER_NODE>
        <PES_PER_NODE>16</PES_PER_NODE>
        <comment>none</comment>
        <ntasks>
          <ntasks_atm>32</ntasks_atm>
          <ntasks_rof>32</ntasks_rof>
          <ntasks_ocn>32</ntasks_ocn>
          <ntasks_ice>32</ntasks_ice>
          <ntasks_cpl>32</ntasks_cpl>
          <ntasks_lnd>32</ntasks_lnd>
          <ntasks_glc>32</ntasks_glc>
          <ntasks_wav>32</ntasks_wav>
        </ntasks>
        <nthrds>
          <nthrds_atm>1</nthrds_atm>
          <nthrds_lnd>1</nthrds_lnd>
          <nthrds_rof>1</nthrds_rof>
          <nthrds_ice>1</nthrds_ice>
          <nthrds_ocn>1</nthrds_ocn>
          <nthrds_glc>1</nthrds_glc>
          <nthrds_wav>1</nthrds_wav>
          <nthrds_cpl>1</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_rof>0</rootpe_rof>
          <rootpe_ice>0</rootpe_ice>
          <rootpe_ocn>0</rootpe_ocn>
          <rootpe_cpl>0</rootpe_cpl>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_glc>0</rootpe_glc>
          <rootpe_wav>0</rootpe_wav>
        </rootpe>
      </pes>

      <!-- C compset, gx1v6 or gx1v7 resolution on yellowstone:
           _POP gets 128x1
           all other components get 16x1 -->
      <pes pesize="any" compset="_DATM.*_DICE.*_POP2[^%]">
        <MAX_TASKS_PER_NODE>16</MAX_TASKS_PER_NODE>
        <PES_PER_NODE>16</PES_PER_NODE>
        <comment>none</comment>
        <ntasks>
          <ntasks_atm>16</ntasks_atm>
          <ntasks_rof>16</ntasks_rof>
          <ntasks_ocn>128</ntasks_ocn>
          <ntasks_ice>16</ntasks_ice>
          <ntasks_cpl>16</ntasks_cpl>
          <ntasks_lnd>16</ntasks_lnd>
          <ntasks_glc>16</ntasks_glc>
          <ntasks_wav>16</ntasks_wav>
        </ntasks>
        <nthrds>
          <nthrds_atm>1</nthrds_atm>
          <nthrds_lnd>1</nthrds_lnd>
          <nthrds_rof>1</nthrds_rof>
          <nthrds_ice>1</nthrds_ice>
          <nthrds_ocn>1</nthrds_ocn>
          <nthrds_glc>1</nthrds_glc>
          <nthrds_wav>1</nthrds_wav>
          <nthrds_cpl>1</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_rof>0</rootpe_rof>
          <rootpe_ice>0</rootpe_ice>
          <rootpe_ocn>16</rootpe_ocn>
          <rootpe_cpl>0</rootpe_cpl>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_glc>0</rootpe_glc>
          <rootpe_wav>0</rootpe_wav>
        </rootpe>
      </pes>

      <!-- G compset, gx1v6 or gx1v7 resolution on yellowstone:
           _POP gets 128x1
           _CICE / coupler get 64x1
           All other components get 16x1 -->
      <pes pesize="any" compset="_DATM.*_CICE.*_POP2[^%]">
        <MAX_TASKS_PER_NODE>16</MAX_TASKS_PER_NODE>
        <PES_PER_NODE>16</PES_PER_NODE>
        <comment>none</comment>
        <ntasks>
          <ntasks_atm>16</ntasks_atm>
          <ntasks_rof>16</ntasks_rof>
          <ntasks_ocn>128</ntasks_ocn>
          <ntasks_ice>48</ntasks_ice>
          <ntasks_cpl>48</ntasks_cpl>
          <ntasks_lnd>16</ntasks_lnd>
          <ntasks_glc>16</ntasks_glc>
          <ntasks_wav>16</ntasks_wav>
        </ntasks>
        <nthrds>
          <nthrds_atm>1</nthrds_atm>
          <nthrds_lnd>1</nthrds_lnd>
          <nthrds_rof>1</nthrds_rof>
          <nthrds_ice>1</nthrds_ice>
          <nthrds_ocn>1</nthrds_ocn>
          <nthrds_glc>1</nthrds_glc>
          <nthrds_wav>1</nthrds_wav>
          <nthrds_cpl>1</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_rof>0</rootpe_rof>
          <rootpe_ice>16</rootpe_ice>
          <rootpe_ocn>64</rootpe_ocn>
          <rootpe_cpl>16</rootpe_cpl>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_glc>0</rootpe_glc>
          <rootpe_wav>0</rootpe_wav>
        </rootpe>
      </pes>

      <!-- CECO, gx1v6 or gx1v7 on yellowstone:
           _POP gets 432x1
           all other components get 16x1 -->
      <pes pesize="any" compset="_DATM.*_DICE.*_POP2%ECO">
        <MAX_TASKS_PER_NODE>16</MAX_TASKS_PER_NODE>
        <PES_PER_NODE>16</PES_PER_NODE>
        <comment>none</comment>
        <ntasks>
          <ntasks_atm>16</ntasks_atm>
          <ntasks_rof>16</ntasks_rof>
          <ntasks_ocn>432</ntasks_ocn>
          <ntasks_ice>16</ntasks_ice>
          <ntasks_cpl>16</ntasks_cpl>
          <ntasks_lnd>16</ntasks_lnd>
          <ntasks_glc>16</ntasks_glc>
          <ntasks_wav>16</ntasks_wav>
        </ntasks>
        <nthrds>
          <nthrds_atm>1</nthrds_atm>
          <nthrds_lnd>1</nthrds_lnd>
          <nthrds_rof>1</nthrds_rof>
          <nthrds_ice>1</nthrds_ice>
          <nthrds_ocn>1</nthrds_ocn>
          <nthrds_glc>1</nthrds_glc>
          <nthrds_wav>1</nthrds_wav>
          <nthrds_cpl>1</nthrds_cpl>
        </nthrds>
        <rootpe>
          <rootpe_atm>0</rootpe_atm>
          <rootpe_rof>0</rootpe_rof>
          <rootpe_ice>0</rootpe_ice>
          <rootpe_ocn>16</rootpe_ocn>
          <rootpe_cpl>0</rootpe_cpl>
          <rootpe_lnd>0</rootpe_lnd>
          <rootpe_glc>0</rootpe_glc>
          <rootpe_wav>0</rootpe_wav>
        </rootpe>
      </pes>

      <!-- GECO compset, gx1v6 or gx1v7 resolution on yellowstone:
           _POP gets 432x1
