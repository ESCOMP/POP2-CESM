<?xml version="1.0"?>

<?xml-stylesheet type="text/xsl" href="config_definition.xsl" ?>

<config_definition>

<entry id="ocn_grid" valid_values="gx3v7,gx1v6,tx0.1v2,tx1v1" value="">
Ocean grid used for POP
</entry>

<entry id="use_iage" valid_values="FALSE,TRUE" value="TRUE">
Use ideal age tracer module
</entry>
<entry id="use_cfc" valid_values="FALSE,TRUE" value="FALSE">
Use cfc tracer module
</entry>
<entry id="use_abio" valid_values="FALSE,TRUE" value="FALSE">
Use abio tracer module
</entry>
<entry id="use_ciso" valid_values="FALSE,TRUE" value="FALSE">
Use ciso tracer module
</entry>
<entry id="use_ecosys" valid_values="FALSE,TRUE" value="FALSE">
Use ecosys tracer module
</entry>
<entry id="use_moby" valid_values="FALSE,TRUE" value="FALSE">
Use moby tracer module
</entry>
<entry id="use_irf" valid_values="FALSE,TRUE" value="FALSE">
Use IRF tracer module
</entry>

<entry id="irf_nt" value="0">
Number of IRF tracers
</entry>
<entry id="irf_mode" valid_values="NK_precond,offline_transport" value="NK_precond">
IRF tracer module mode
</entry>

<entry id="cppdefs" value="">
CPPDEF used during POP build
</entry>

</config_definition>
